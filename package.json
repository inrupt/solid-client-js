    "eslint-plugin-license-header": "^0.4.0",
    "fast-check": "^2.2.0",
    "jest": "^27.0.4",
    "license-checker": "^25.0.1",
    "prettier": "2.6.2",
    "rdf-namespaces": "^1.8.0",
    "rollup": "^2.15.0",
