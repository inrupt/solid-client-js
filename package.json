    "./thing/mock": {
      "types": "./dist/thing/mock.d.ts",
      "import": "./dist/thing/mock.mjs"
    },
    "./acl/acl": {
      "types": "./dist/acl/acl.d.ts",
      "import": "./dist/acl/acl.mjs"
    },
    "./acl/agent": {
      "types": "./dist/acl/agent.d.ts",
